
/var/log/dokimion/dokimion_server_4.log {
   daily
   missingok
   rotate 14
   compress
   notifempty
   create 0640 www-data www-data
   sharedscripts
   postrotate
      systemctl reload dokimion_server_4.service
   endscript
}
/var/log/dokimion/dokimion_ui_4.log {
   daily
   missingok
   rotate 14
   compress
   notifempty
   create 0640 www-data www-data
   sharedscripts
   postrotate
      systemctl reload dokimion_ui_4.service
   endscript
}
